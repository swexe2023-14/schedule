<p id="notice"><%= notice %></p>
<div class="show">
<p>
  <strong>内容</strong>
  <%= @event.content %>
</p>

<p>
  <strong>日時</strong>
  <%= @event.start_time.strftime("%m-%d") %>
</p>

<%= link_to '編集', edit_event_path(@event) %>
<%= link_to '消去',event_path(@event.id), data: {"turbo-method": :delete} %>
<%= link_to '戻る', events_path %>
</div>